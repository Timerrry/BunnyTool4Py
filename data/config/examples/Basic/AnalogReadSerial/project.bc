{"author":"timmery","board":"ArduinoUNO","createdAt":1531280028,"files":[{"content":"from mpython import *           # 导入mpython模块\n\np0 = MPythonPin(0, PinMode.ANALOG)     # 实例化MPythonPin,将P0设置为\"PinMode.ANALOG\"模式\nwhile True:\n    value = p0.read_analog()          # 读取P0引脚模拟量\n    oled.DispChar(\"analog:%d\" %value,30,20)  # 通过屏幕打印读取到得值\n    oled.show()\n    oled.fill(0)","language":"cpp","main":false,"name":"AnalogReadSerial.py"}],"index":0,"name":"AnalogReadSerial","updatedAt":1556623191}
